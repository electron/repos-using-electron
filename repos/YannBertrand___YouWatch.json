  "packageJSON": {
    "name": "youwatch",
    "productName": "YouWatch",
    "version": "0.4.2",
    "description": "An improved YouTube desktop client",
    "license": "MIT",
    "repository": "YannBertrand/YouWatch",
    "main": "dist/app.js",
    "author": {
      "name": "Yann Bertrand",
      "email": "yann@bertrand.com",
      "url": "yann-bertrand.fr"
    },
    "scripts": {
      "start": "node_modules/.bin/gulp",
      "lint": "node_modules/.bin/xo",
      "test": "npm run lint",
      "clean": "rm -rf dist; rm -f ~/.config/configstore/YouWatch.json; rm -rf ~/Library/Application\\ Support/YouWatch",
      "build": "npm run build:osx && npm run build:linux && npm run build:windows",
      "build:osx": "node_modules/.bin/electron-packager . --overwrite --out=build --prune --platform=darwin --arch=x64 --app-version=$npm_package_version --icon=static/icon.icns",
      "build:linux": "node_modules/.bin/electron-packager . --overwrite --out=build --ignore='^/(build|src|media)$' --prune --platform=linux --arch=x64 --app-version=$npm_package_version",
      "build:windows": "electron-packager . --overwrite --out=build --prune --platform=win32 --arch=ia32 --app-version=%npm_package_version% --icon=static/icon.png"
    },
    "dependencies": {
      "async": "^2.0.1",
      "bootstrap": "^4.0.0-alpha.4",
      "electron-config": "^0.2.1",
      "electron-debug": "^1.0.0",
      "electron-is-dev": "^0.1.2",
      "font-awesome": "^4.6.3",
      "googleapis": "^14.1.0",
      "hapi": "^15.0.3",
      "inert": "^4.0.2",
      "jquery": "^3.1.1",
      "lazysizes": "^2.0.2",
      "lodash": "^4.15.0",
      "react": "^15.1.0",
      "react-dom": "^15.1.0",
      "tcp-port-used": "^0.1.2",
      "tether": "^1.3.2",
      "youtube-iframe": "^1.0.2",
      "youtube-regex": "^1.0.5",
      "z-switch": "^1.0.0"
    },
    "devDependencies": {
      "babel-preset-es2015": "^6.9.0",
      "babel-preset-react": "^6.5.0",
      "devtron": "^1.1.2",
      "electron": "^1.4.3",
      "electron-connect": "^0.6.0",
      "electron-packager": "^8.0.0",
      "eslint-config-xo-react": "^0.10.0",
      "eslint-plugin-react": "^6.4.1",
      "gulp": "^3.9.1",
      "gulp-babel": "^6.1.2",
      "gulp-cached": "^1.1.0",
      "gulp-debug": "^2.1.2",
      "gulp-notify": "^2.2.0",
      "gulp-plumber": "^1.1.0",
      "gulp-replace-task": "^0.11.0",
      "gulp-sass": "^2.3.2",
      "gulp-sourcemaps": "^2.1.1",
      "xo": "^0.17.0"
    },
    "xo": {
      "ignores": [
        "build/**/*",
        "media/**/*",
        "static/**/*"
      ],
      "esnext": true,
      "space": 2,
      "rules": {
        "comma-dangle": [
          "warn",
          "always-multiline"
        ],
        "no-multi-spaces": "off",
        "curly": "off",
        "babel/object-curly-spacing": "off",
        "key-spacing": "off",
        "no-multiple-empty-lines": "off",
        "arrow-parens": [
          "error",
          "always"
        ],
        "padded-blocks": [
          "warn"
        ],
        "unicorn/catch-error-name": [
          "error",
          {
            "name": "error"
          }
        ],
        "callback-return": "warn",
        "object-shorthand": [
          "error",
          "always",
          {
            "avoidQuotes": true
          }
        ],
        "quote-props": "warn",
        "object-curly-spacing": [
          "warn",
          "always"
        ],
        "brace-style": [
          "error",
          "1tbs",
          {
            "allowSingleLine": true
          }
        ],
        "eol-last": "error",
        "react/jsx-space-before-closing": [
          "warn",
          "always"
        ],
        "react/jsx-handler-names": "off",
        "import/no-unresolved": [
          "warn",
          {
            "ignore": [
              ".react$"
            ]
          }
        ]
      },
      "overrides": [
        {
          "files": "**/*.js",
          "envs": [
            "node"
          ]
        },
        {
          "files": "src/client/**/*.js",
          "extends": "xo-react",
          "globals": [
            "window",
            "document",
            "YT"
          ],
          "envs": [
            "browser"
          ]
        }
      ]
    },
    "other": {}
  },
