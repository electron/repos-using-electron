          {
            "x": 130,
            "y": 150,
            "type": "file"
          }
        ]
      },
      "files": [
        "dist/",
        "node_modules/",
        "main.js",
        "main.js.map",
        "package.json",
        "html/",
        "pdfjs/",
        "config/"
      ],
      "appx": {
        "displayName": "Ashley"
      },
      "nsis": {
        "oneClick": true
      },
      "linux": {
