      "eslint-plugin-react": "~6.7.0",
      "flow-remove-types": "^1.1.2",
      "gl": "^4.0.3",
      "glob": "^7.1.1",
      "jsdom": "~9.9.1",
      "module-alias": "^2.0.0",
      "path-exists": "^3.0.0",
