      "request": "^2.81.0",
      "semver": "^5.3.0",
      "unzipper": "^0.8.8",
      "vue": "^2.1.3"
    },
    "devDependencies": {
      "coffee-script": "^1.11.1",
      "del": "^2.2.2",
      "electron": "^1.6.2",
      "electron-builder": "^17.0.1",
      "gulp": "^3.9.1",
      "gulp-changed": "^1.3.2",
      "gulp-coffee": "^2.3.2",
      "gulp-haml": "^0.1.6",
      "gulp-notify": "^2.2.0",
      "gulp-plumber": "^1.1.0",
      "gulp-sass": "^2.3.2",
      "gulp-yaml": "^1.0.1"
    },
    "build": {
