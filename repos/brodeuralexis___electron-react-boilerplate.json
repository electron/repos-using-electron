  "packageJSON": {
    "name": "electron-react-boilerplate",
    "version": "1.0.0",
    "description": "A React and Electron boilerplate",
    "main": "bin/main.js",
    "license": "MIT",
    "scripts": {
      "start": "NODE_ENV=prod yarn electron bin/main.js --",
      "start:dev": "NODE_ENV=dev yarn start --",
      "packager": "NODE_ENV=prod yarn webpack --",
      "packager:dev": "NODE_ENV=dev yarn webpack-dev-server -- --hot --inline",
      "test": "NODE_ENV=test yarn jest --",
      "test:watch": "yarn test -- --watch"
    },
    "dependencies": {
      "electron": "^1.6.1",
      "react": "^15.4.2",
      "react-dom": "^15.4.2",
      "react-hot-loader": "next"
    },
    "devDependencies": {
      "babel-core": "^6.23.1",
      "babel-loader": "^6.4.0",
      "babel-plugin-contracts": "^1.1.1",
      "babel-polyfill": "^6.23.0",
      "babel-preset-es2015": "^6.22.0",
      "babel-preset-react": "^6.23.0",
      "babel-preset-stage-0": "^6.22.0",
      "chai": "^3.5.0",
      "css-loader": "^0.26.2",
      "electron-react-devtools": "^0.4.0",
      "eslint": "^3.17.1",
      "eslint-config-standard": "^7.0.1",
      "eslint-config-standard-react": "^4.3.0",
      "eslint-loader": "^1.6.3",
      "eslint-plugin-import": "^2.2.0",
      "eslint-plugin-promise": "^3.5.0",
      "eslint-plugin-react": "^6.10.0",
      "eslint-plugin-standard": "^2.1.1",
      "extract-text-webpack-plugin": "^2.1.0",
      "file-loader": "^0.10.1",
      "html-webpack-plugin": "^2.28.0",
      "identity-obj-proxy": "^3.0.0",
      "jest": "^19.0.2",
      "json-loader": "^0.5.4",
      "node-sass": "^4.5.0",
      "sass-loader": "^6.0.3",
      "style-loader": "^0.13.2",
      "url-loader": "^0.5.8",
      "webpack": "^2.2.1",
      "webpack-dev-server": "^2.4.1",
      "webpack-target-electron-renderer": "^0.4.0",
      "yaml-loader": "^0.4.0"
    },
    "jest": {
      "verbose": true,
      "modulePaths": [
        "<rootDir>/src"
      ],
      "moduleNameMapper": {
        "\\.s?css$": "identity-obj-proxy"
      },
      "transform": {
        "\\.(?:ico|gif|png|jpg|jpeg|webp)$": "<rootDir>/bin/identity-path-transformer"
      },
      "globals": {
        "__ENV__": "test",
        "__PROD__": false,
        "__DEV__": false,
        "__TEST__": true,
        "process.env.NODE_ENV": "test"
      }
