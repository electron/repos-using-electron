    "scripts": {
      "bootstrap": "docker exec -it cozydesktop_stack_1 bootstrap-cozy-desktop.sh",
      "bootstrap:remote": "babel-node ./dev/remote/generate-test-env.js",
      "build": "babel --source-maps --out-dir lib/ src/",
      "clean": "rm -rf lib/ && rm -rf tmp/",
      "dev-storage": "yarn stack -- tree -aC /storage/dev.cozy.tools:8080",
      "eslint": "./node_modules/.bin/eslint './src/**/*.js' './test/**/*.js'",
      "fauxton": "babel-node ./src/bin/fauxton.js",
      "flow": "flow status --quiet --show-all-errors",
      "lint": "npm run eslint && npm run flow",
      "mocha": "env-cmd .env.test mocha",
      "release:build": "yarn release:check && yarn && yarn build",
      "release:check": "babel-node ./dev/check-release-branch.js",
      "repl": "cross-env DEBUG=1 COZY_DESKTOP_HEARTBEAT=5000 babel-node ./src/bin/repl.js",
      "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
      "start": "cross-env COZY_DESKTOP_HEARTBEAT=5000 babel-node ./src/bin/cli.js",
      "stack": "docker exec -it cozydesktop_stack_1",
      "test": "installed-check && npm run test-unit && npm run test-integration",
      "test-coverage": "installed-check && npm run test-unit-coverage && npm run test-integration",
      "test-integration": "cross-env NODE_ENV=test mocha test/integration/v3",
      "test-unit": "yarn mocha -- test/unit/",
      "test-unit-coverage": "cross-env NODE_ENV=test nyc mocha test/unit/",
      "watch": "npm run build -- --watch"
    },
