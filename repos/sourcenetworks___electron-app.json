  "packageJSON": {
    "name": "source-client",
    "version": "0.0.1",
    "description": "source client",
    "main": "main.js",
    "scripts": {
      "pretest": "npm run lint",
      "test": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test/**/*.spec.js",
      "test-watch": "npm test -- --watch",
      "lint": "eslint renderer test *.js",
      "hot-server": "node server.js",
      "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
      "start": "cross-env NODE_ENV=production electron ./",
      "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./",
      "dev": "concurrently --kill-others \"npm run webpack:dev\" \"npm run hot-server\" \"npm run start-hot\"",
      "package-all": "npm run package -- --all",
      "webpack:prod": "webpack --config webpack.config.production.js",
      "webpack:dev": "webpack --config webpack.config.development.js"
    },
    "bin": {
      "electron": "./node_modules/.bin/electron"
    },
