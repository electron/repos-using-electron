        "main.js.map",
        "package.json"
      ],
      "win": {
        "target": "nsis"
      },
      "linux": {
        "target": [
          "deb",
          "AppImage"
        ]
      },
      "directories": {
        "buildResources": "resources",
        "output": "release"
      }
    },
