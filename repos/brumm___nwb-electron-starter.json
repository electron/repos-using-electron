  "packageJSON": {
    "name": "nwb-electron-starter",
    "productName": "My Electron App",
    "version": "1.0.0",
    "description": "Describe nwb-electron-starter here",
    "main": "dist/index.js",
    "private": true,
    "scripts": {
      "build": "nwb build && babel main -d dist && npm run package",
      "watch-electron-main": "babel main -d dist -w",
      "clean": "nwb clean",
      "start": "babel main -d dist && npm run server",
      "server": "node dev-server.js --run-electron",
      "test": "nwb test",
      "test:watch": "nwb test --server",
      "package": "rm -rf ./release && electron-packager --overwrite --asar='true' --ignore='src|main|tests' --prune --platform=darwin --arch=x64 --out='release' ."
    },
    "dependencies": {
