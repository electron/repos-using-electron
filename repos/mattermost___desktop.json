      "electron-connect": "^0.6.1",
      "eslint": "^3.16.1",
      "eslint-plugin-react": "^6.10.0",
      "mocha": "^3.2.0",
      "mocha-circleci-reporter": "0.0.2",
      "npm-run-all": "^4.0.2",
      "spectron": "~3.6.0",
      "url-loader": "^0.5.8",
      "webpack": "^2.2.1",
      "webpack-dev-server": "^2.4.1",
      "webpack-merge": "^3.0.0"
    },
