        "target": [
          "squirrel"
        ]
      },
      "linux": {
        "target": [
          "zip"
        ]
      },
      "mac": {
        "category": "public.app-category.finance",
        "target": [
          "zip"
        ]
      },
      "directories": {
        "output": "releases",
        "app": "dist"
      }
    },
    "other": {
