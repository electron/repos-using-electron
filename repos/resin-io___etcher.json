      "assets/osx",
      "appveyor.yml",
      "bower.json",
      "screenshot.png",
      "tests",
      "etcher-release",
      "lib/scss",
      "bin/etcher"
    ],
    "shrinkwrapIgnore": [
      "macos-alias",
      "fs-xattr",
      "ds-store",
      "diskpart",
      "appdmg"
