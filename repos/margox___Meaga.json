  "packageJSON": {
    "name": "vue-music-player",
    "version": "1.0.2",
    "description": "A Small Music Player Powered by Vue and Electron.",
    "author": "Margox <margox@foxmail.com>",
    "private": true,
    "scripts": {
      "start": "node build/dev-server.js",
      "build": "rm -rf dist && node build/build.js",
      "dev": "NODE_ENV=development electron ./electron/",
      "prod": "NODE_ENV=development electron ./dist/",
      "package": "npm run build && electron-packager ./dist --electron-version=1.6.2 --out=./packaged --overwrite --icon=./electron/static/images/app_icon.png",
      "dist": "npm run build && build",
      "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
      "test": "npm run unit",
      "lint": "eslint --ext .js,.vue src test/unit/specs"
    },
    "build": {
      "appId": "app.meaga",
      "directories": {
        "buildResources": "./build_assets",
        "app": "dist",
        "output": "packaged"
      }
    },
