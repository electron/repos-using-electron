    "name": "angular-electron-cherry-pie",
    "version": "0.0.1",
    "author": "AlexPlex",
    "licence": "UNLICENSED",
    "private": true,
    "directories": {
      "app": "./client/public",
      "output": "./electron-dist"
    },
    "build": {
      "appId": "com.example.electron-app"
    },
    "scripts": {
      "postinstall": "install-app-deps",
      "start": "npm run build:dev",
      "server": "lite-server -c server.config.json",
      "build:dev": "set NODE_ENV=development&& npm run clean && npm run copy-files && webpack --config webpack.dev.config.js --watch",
      "build:prod": "set NODE_ENV=production&& npm run copy-files && npm run aot && webpack --config webpack.prod.config.js",
      "copy-files": "cp client/app/package.json client/public/",
      "clean": "rimraf ./ngfactory",
      "aot": "ngc-w -p tsconfig.json --webpack webpack.prod.config.js",
      "electron": "electron client/public/",
      "electron:server": "concurrently \"npm run server\" \"npm run electron\"",
      "electron:pack": "npm run build:prod && build --dir",
      "electron:dist": "npm run build:prod && build"
    },
