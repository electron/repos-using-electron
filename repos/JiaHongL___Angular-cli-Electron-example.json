  "packageJSON": {
    "name": "angular-cli-electron-example",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
      "postinstall": "cpx webpack-config.js node_modules/@angular/cli/models",
      "ng": "ng",
      "start": "ng serve",
      "build": "ng build",
      "test": "ng test",
      "lint": "ng lint",
      "e2e": "ng e2e",
      "clean": "rm -rf ./dist && rm -rf ./release",
      "build-electron-dev": "ng build --base-href . && cpx \"src/electron/dev/*.{js,json}\" dist",
      "build-electron-prod": "ng build -prod --base-href . && cpx \"src/electron/prod/*.{js,json}\" dist",
      "electron": "npm run build-electron-dev && concurrently \"ng serve\" \"electron dist\"",
      "build_mac": "npm run build-electron-prod && electron-packager ./dist ng-electron-example --platform=darwin --arch=x64 --out=./release --version=1.6.2 --icon=tools.icns --asar=true --ignore=node_modules/electron-* --overwrite",
      "build_win32": "npm run build-electron-prod && electron-packager ./dist ng-electron-example --platform=win32 --arch=x64 --out=./release --version=1.6.2 --icon=tools.ico --asar=true --ignore=node_modules/electron-* --overwrite",
      "build_linux": "npm run build-electron-prod && electron-packager ./dist ng-electron-example --platform=linux --arch=x64 --out=./release --version=1.6.2 --icon=tools.png --asar=true --ignore=node_modules/electron-* --overwrite"
    },
    "private": true,
    "dependencies": {
      "@angular/common": "^4.0.0",
      "@angular/compiler": "^4.0.0",
      "@angular/core": "^4.0.0",
      "@angular/forms": "^4.0.0",
