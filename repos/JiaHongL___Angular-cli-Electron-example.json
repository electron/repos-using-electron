      "@types/electron": "^1.4.34",
      "core-js": "^2.4.1",
      "rxjs": "^5.1.0",
      "zone.js": "^0.8.4"
    },
    "devDependencies": {
      "@angular/cli": "1.0.0",
      "@angular/compiler-cli": "^4.0.0",
      "@types/electron": "^1.4.33",
      "@types/jasmine": "2.5.38",
      "@types/node": "~6.0.60",
      "codelyzer": "~2.0.0",
