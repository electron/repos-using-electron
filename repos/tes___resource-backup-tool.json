  "packageJSON": {
    "name": "resource-backup-tool",
    "version": "1.1.0",
    "description": "A tool to backup my teaching resources",
    "main": "main.js",
    "productName": "Resource Backup Tool",
    "scripts": {
      "build": "tsc -p src --outDir app",
      "build:watch": "tsc -p src --outDir app -w",
      "build-tests": "cp -r data test && tsc -p tests_src --outDir test --jsx react",
      "build-tests:watch": "tsc -p tests_src --outDir test -w --jsx react",
      "start-dev": "electron .",
      "start": "NODE_ENV=production electron .",
      "test": "npm run build-tests && NODE_ENV=test mocha --recursive test/tests_src/**/*.tests.js",
      "compile": "npm run build && electron-packager . Resource-Backup-Tool --all --overwrite --out ./compiled",
      "compile-linux": "npm run build && electron-packager . Resource-Backup-Tool --platform=linux --arch=x64 --overwrite --out ./compiled",
      "compile-win32": "npm run build && electron-packager . Resource-Backup-Tool --platform=win32 --arch=ia32 --overwrite --out ./compiled",
      "precommit": "npm run lint",
      "lint": "tslint src/**.ts?"
    },
    "repository": "https://github.com/tes/resource-backup-tool",
