    "main": "src/main.js",
    "scripts": {
      "build": "npm run pack -- --platform=linux,darwin,win32",
      "build:linux": "npm run pack -- --platform=linux",
      "build:mac": "npm run pack -- --platform=darwin --icon=assets/Replay.icns",
      "build:windows": "npm run pack -- --platform=win32",
      "extract-strings": "node ./bin/extract-strings -i src -o src/i18n/strings.pot",
      "pack": "electron-packager .tmp --electron-version=$npm_package_devDependencies_electronPrebuilt --out=dist --prune --overwrite --arch=x64",
      "prepack": "npm install && node ./bin/preprocess -i src -o .tmp",
      "postpack": "rm -rf .tmp",
      "start": "electron .",
      "start:debug": "DEBUG=1 electron ."
    },
