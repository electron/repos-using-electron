  "packageJSON": {
    "name": "skypager-monorepo",
    "private": "true",
    "version": "17.1.0",
    "description": "skypager active publishing framework",
    "scripts": {
      "console": "skypager console",
      "install:deps": "yarn add skypager skypager-project-types-react skypager-project-types-electron-app --save",
      "update:deps": "./bin/update-deps",
      "test": "lerna run test --ignore=skypager-vscode-extension",
      "test:watch": "parallelshell './bin/devwatch skypager' './bin/devwatch skypager-project' './bin/devwatch skypager-util' './bin/devwatch skypager-helper' './bin/devwatch skypager-registry'",
      "build:watch": "parallelshell './bin/devbuild-watch skypager' './bin/devbuild-watch skypager-project' './bin/devbuild-watch skypager-util' './bin/devbuild-watch skypager-helper' './bin/devbuild-watch skypager-registry'",
      "build": "lerna run build --ignore=skypager-vscode-extension",
      "lint": "exit 0",
      "app:start:prod": "NODE_ENV=production electron ./app/main/index.js --app-path=app --runtime-root=app/renderer",
      "app:start": "NODE_ENV=development electron ./app/main/index.js --repl --app-path=app --runtime-root=app/renderer --show-dev-tools",
      "app:start:hot": "NODE_ENV=development electron ./app/main/index.js --repl --hot --app-path=app --runtime-root=app/renderer",
      "app:dev": "npm run app:build:main && npm run app:start:hot",
      "preapp:start:dev": "npm run app:build:helpers && npm run app:build:renderer:dll",
      "app:start:dev": "npm run app:start && parallelshell 'npm run app:build:main:watch' 'npm run app:build:renderer:watch'",
      "app:clean": "rm -rf npm-debug.* app/main app/renderer",
      "app:build": "npm run app:clean && npm run app:build:helpers && npm run app:build:main && npm run app:build:renderer && npm run app:build:renderer:dll",
      "app:build:helpers": "skypager build electron-app-main --disable-copy-plugin --out app/main/helpers ./src/helpers/index.js",
      "app:build:main": "skypager build electron-app-main --out app/main ./src/main/index.js",
      "app:build:main:watch": "skypager build electron-app-main --watch --development --out app/main ./src/main/index.js",
      "app:build:renderer": "skypager build electron-renderer --production --dll-name=electron-renderer --out app/renderer --in ./src/renderer/index.js",
      "app:build:renderer:dll": "skypager build electron-renderer --production --dll-name=electron-renderer --disable-copy-plugin --disable-dll-plugin --generate-dll --out app/renderer --in ./src/renderer/index.js",
      "app:build:renderer:watch": "skypager build electron-renderer --watch --production --dll-name=electron-renderer --out app/renderer --in ./src/renderer/index.js"
    },
    "keywords": [
      "skypager",
      "active",
      "publishing",
      "reactive",
      "documents",
      "react",
      "markdown"
    ],
    "homepage": "https://github.com/skypager/skypager#readme",
    "license": "MIT",
    "author": "Jon Soeder <jon@chicago.com> (https://twitter.com/soederpop)",
    "skypager": {
      "paths": {
        "output": "app",
        "bundles": "app/renderer",
        "public": "app/renderer",
        "source": "src",
        "cache": "node_modules/.cache/skypager-monorepo",
        "library": "lib",
        "ast": "dist/ast",
        "docs": "docs",
        "data": "data",
        "logs": "log",
        "temp": "tmp",
        "main": "skypager.js",
        "manifest": "package.json"
      },
      "models": {
        "Package": {
          "routes": [
            "packages/:name/package.json"
          ]
        }
      }
    },
    "files": [
      "dist/docs",
      "lib",
      "src",
      "test",
      "package.json",
      "Makefile",
      "index.json",
      "README.md",
      "bin"
    ],
    "main": "index.js",
    "repository": "https://github.com/skypager/skypager",
    "devDependencies": {
      "babel-core": "^6.22.0",
