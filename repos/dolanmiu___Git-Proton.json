  "packageJSON": {
    "name": "git-proton",
    "version": "0.2.0",
    "description": "An open source Electron based Git GUI tool",
    "scripts": {
      "start-web": "webpack-dev-server --open --inline --progress --port 8080",
      "start": "npm run build && electron .",
      "build-main": "webpack --config config/webpack.main.js --progress --profile --bail --display-error-details",
      "build-electron": "webpack --config config/webpack.prod.js --progress --profile --bail --display-error-details",
      "build": "rimraf dist && npm run build-electron && npm run build-main",
      "package": "npm run build && npm run prepare && ./node_modules/electron-packager/cli.js dist App --platform=darwin --arch=all --out=dist-app --overwrite",
      "electron": "electron .",
      "karma": "karma start 'config/karma.conf.js'",
      "lint": "tslint --project .",
      "test": "npm run karma && npm run lint"
    },
    "licenses": [
