    "version": "0.0.1",
    "description": "Electron + React + Mobx + TypeScript + Webpack + TypeStyle Boilerplate",
    "main": "./app/main.js",
    "scripts": {
      "start": "cross-env NODE_ENV=development electron .",
      "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod",
