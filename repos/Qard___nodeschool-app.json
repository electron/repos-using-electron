      "pack:windows": "electron-packager . --platform=win32 --arch=ia32 --icon=assets/nodeschool.ico --overwrite --ignore=/out/ --prune=true --out=out ",
      "pack": "npm run pack:mac && npm run pack:linux && npm run pack:windows"
    },
