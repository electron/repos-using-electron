  "packageJSON": {
    "name": "project-sothis",
    "productName": "ProjectSothis",
    "version": "0.1.0",
    "description": "Modern companion app for Elite Dangerous.",
    "main": "./build/main/index.js",
    "scripts": {
      "build": "npm run clean && npm run build-main && npm run build-renderer",
      "build-main": "cross-env NODE_ENV=production node -r babel-core/register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js",
      "build-renderer": "cross-env NODE_ENV=production node -r babel-core/register ./node_modules/webpack/bin/webpack --config webpack.config.prod.js",
      "clean": "npm run clean:dist && npm run clean:pkg",
      "clean:dist": "rimraf ./build",
      "clean:pkg": "rimraf ./package",
      "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
      "hot-server": "cross-env NODE_ENV=development node -r babel-core/register server.js",
      "lint": "cross-env NODE_ENV=test standard",
      "package": "npm run build && npm run package:win",
      "package:win": "cross-env NODE_ENV=production electron-packager . --prune --platform=win32 --out=./package",
      "start": "cross-env NODE_ENV=production electron .",
      "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-core/register -r babel-polyfill ./app/main/index",
      "test": "cross-env NODE_ENV=test mocha -R spec --compilers js:babel-core/register --require ignore-styles"
    },
    "author": "Resi Respati <resir014@gmail.com>",
    "repository": "https://github.com/project-sothis/project-sothis",
