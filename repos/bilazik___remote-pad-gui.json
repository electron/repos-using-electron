  "packageJSON": {
    "name": "remote-pad-gui",
    "version": "1.0.0",
    "description": "Electron app to manage both Remote Pad and Remote Pad Server",
    "scripts": {
      "build": "npm run pack && build",
      "build:win32": "npm run pack && build --win --x64",
      "build:dir": "npm run pack && build --dir",
      "dev": "node tasks/runner.js",
      "e2e": "npm run pack && mocha test/e2e",
      "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter app test",
      "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix app test",
      "pack": "npm run pack:main && npm run pack:renderer",
      "pack:main": "cross-env NODE_ENV=production webpack -p --progress --colors --config webpack.main.config.js",
      "pack:renderer": "cross-env NODE_ENV=production webpack -p --progress --colors --config webpack.renderer.config.js",
      "test": "npm run unit && npm run e2e",
      "unit": "cross-env BABEL_ENV=testing-unit karma start test/unit/karma.conf.js",
      "vue:route": "node tasks/vue/route.js",
      "vuex:module": "node tasks/vuex/module.js",
      "postinstall": "cd app && yarn",
      "docs:prepare": "gitbook install docs",
      "docs:watch": "npm run docs:prepare && gitbook serve docs",
      "docs:cleanup": "rm -rf docs/_book",
      "docs:build": "npm run docs:prepare && npm run docs:cleanup && gitbook build docs",
      "docs:publish": "npm run docs:build && cd docs/_book && git init && git commit --allow-empty -m 'Update docs' && git checkout -b gh-pages && git add . && git commit -am 'Update docs' && git push git@github.com:comsolid/remote-pad-gui gh-pages --force"
    },
    "build": {
      "productName": "remote-pad-gui",
      "appId": "org.comsolid.remote-pad-gui",
      "dmg": {
        "contents": [
          {
            "x": 410,
            "y": 150,
            "type": "link",
            "path": "/Applications"
          },
          {
            "x": 130,
            "y": 150,
            "type": "file"
          }
        ]
      },
      "files": [
        "dist/",
        "node_modules/",
        "package.json"
      ],
      "mac": {
        "category": "public.app-category.tools",
        "icon": "app/icons/icon.icns"
      },
      "win": {
        "icon": "app/icons/icon.ico",
        "target": [
          "zip"
        ]
      },
      "linux": {
        "target": [
          "AppImage",
          "deb"
        ],
        "category": "Game",
        "executableName": "remote-pad-gui",
        "desktop": {
          "Name": "Remote Pad GUI"
        }
      },
      "extraFiles": [
        "services"
      ]
    },
    "author": "Átila Camurça <camurca.home@gmail.com>",
    "license": "GPL-3.0",
    "devDependencies": {
