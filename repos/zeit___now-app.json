  "packageJSON": {
    "name": "now-desktop",
    "productName": "Now",
    "description": "Realtime global deployments",
    "version": "1.2.2",
    "author": {
      "name": "Zeit, Inc.",
      "email": "team@zeit.co"
    },
    "repository": "zeit/now-desktop",
    "main": "main/index.js",
    "scripts": {
      "start": "electron main",
      "build": "next build renderer",
      "precommit": "lint-staged",
      "test": "xo",
      "pack": "npm run build && build --dir",
      "dist": "npm run build && build"
    },
    "build": {
      "appId": "co.zeit.now",
      "asar": false,
      "win": {
        "target": [
          "squirrel"
        ],
        "icon": "main/static/icons/windows.ico"
      },
      "squirrelWindows": {
        "loadingGif": "main/static/install-spinner.gif"
      },
      "mac": {
        "category": "public.app-category.developer-tools",
        "icon": "main/static/icons/mac.icns",
        "extendInfo": {
          "LSUIElement": 1
        }
      },
      "files": [
        "main",
        "node_modules"
      ],
      "extraResources": [
        {
          "from": "renderer",
          "to": "app/renderer",
          "filter": [
            ".next",
            "static"
          ]
        }
      ]
    },
    "lint-staged": {
      "*.js": [
        "npm run lint",
        "prettier --single-quote --no-semi --write",
        "git add"
      ]
    },
    "xo": {
      "extends": [
        "prettier",
        "prettier/react",
        "plugin:react/recommended"
      ],
      "env": [
        "browser",
        "node"
      ],
      "rules": {
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0
      }
    },
    "license": "MIT",
    "dependencies": {
      "app-root-path": "2.0.1",
      "async-retry": "0.3.0",
      "chalk": "1.1.3",
      "child-process-promise": "2.2.1",
      "deglob": "2.1.0",
      "download": "5.0.3",
      "electron-config": "0.2.1",
      "electron-is-dev": "0.1.2",
      "electron-lets-move": "0.0.5",
      "electron-routes": "1.0.0",
      "electron-squirrel-startup": "1.0.0",
      "email-providers": "1.0.0",
      "first-run": "1.2.0",
      "fix-path": "2.1.0",
      "fs-promise": "2.0.2",
      "istextorbinary": "2.1.0",
      "load-licenses": "0.2.1",
      "mkdirp": "0.5.1",
      "moment": "2.18.1",
      "ms": "1.0.0",
      "next": "2.1.1",
      "node-fetch": "1.6.3",
      "now-client": "0.7.0",
      "path-exists": "3.0.0",
      "path-type": "2.0.0",
