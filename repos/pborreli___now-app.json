  "packageJSON": {
    "repository": "zeit/now-app",
    "scripts": {
      "start": "electron app/dist/main",
      "postinstall": "install-app-deps && webpack",
      "dev": "webpack -w",
      "clean": "chmod u+x dist && rm -rf node_modules app/node_modules dist app/dist",
      "build": "NODE_ENV=production webpack",
      "test": "npm run pack && xo && ava",
      "pack": "npm run build && build --dir"
    },
    "build": {
      "appId": "co.zeit.now",
      "category": "public.app-category.developer-tools",
      "asar": false
    },
    "xo": {
      "extends": "xo-react",
      "esnext": true,
      "space": true,
      "semicolon": false,
      "env": [
        "browser",
        "node"
      ],
      "rules": {
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "ava/no-ignored-test-files": 0,
        "ava/no-async-fn-without-await": 0,
        "react/jsx-filename-extension": 0,
        "react/no-danger": 0,
        "no-unneeded-ternary": 0,
        "babel/no-await-in-loop": 0,
        "complexity": 0
      },
      "ignores": [
        "app/**"
      ]
    },
    "greenkeeper": {
      "emails": false
    },
    "license": "MIT",
    "devDependencies": {
      "autosize-input": "0.2.1",
      "ava": "0.16.0",
