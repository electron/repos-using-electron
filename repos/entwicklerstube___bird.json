  "packageJSON": {
    "name": "bird",
    "version": "0.0.1",
    "description": "Follow the internet",
    "main": "pack/init.js",
    "repository": "git@github.com:entwicklerstube/bird.git",
    "author": "michaelzoidl <github@michaelzoidl.com>",
    "license": "MIT",
    "scripts": {
      "start": "ENV=development electron -r babel-register src/init",
      "storybook": "start-storybook -p 9001 -c config/storybook",
      "server": "babel-node src/server.js",
      "channel": "babel-node src/channel/twitter",
      "create": "babel-node ./bin/create",
      "mocha": "mocha src/**/__test__/*.js --require config/mocha --compilers js:babel-core/register",
      "standard": "standard --env mocha",
      "nyc": "nyc --require babel-core/register --require './config/mocha' mocha src/**/__test__/*.js",
      "coverage": "yarn run nyc && nyc report --reporter=text-lcov | coveralls",
      "pack-webpack": "rm -rf pack && mkdir pack && NODE_ENV=production webpack --config config/webpack/index.js",
      "pack-babel": "babel src -d pack --ignore component,reducer,server.js,static,__test__,src/index.js",
      "pack-copy": "cp src/render/index.html pack/render/index.html && cp -R src/build pack/build",
      "pack": "npm-run-all pack-webpack pack-babel pack-copy",
      "build": "yarn run pack && build",
      "release": "yarn run pack && build --draft"
    },
    "build": {
      "appId": "com.entwicklerstube.bird",
      "mac": {
        "category": "public.app-category.productivity"
      },
      "extraResources": "build"
    },
    "babel": {
      "presets": [
        "latest",
        "react",
        "stage-2"
      ],
      "plugins": [
        "transform-decorators"
