  "packageJSON": {
    "name": "idai-field-client",
    "description": "iDAI.field 2 Client",
    "main": "main.js",
    "version": "0.11.3",
    "license": "Apache-2.0",
    "scripts": {
      "prebuild": "rimraf css/* && rimraf src/* && rimraf fonts/*",
      "build": "gulp create-configs && gulp compile",
      "start": "electron .",
      "test": "jasmine JASMINE_CONFIG_PATH=test/node/config/jasmine.json && karma start test/browser/config/karma.conf.js",
      "pree2e": "rimraf test/e2e-screenshots/*.png",
      "e2e": "node test/e2e/spectron.js",
      "server": "gulp webserver-watch",
      "make-dist": "gulp versioning --build $1",
      "package": "build -mw --publish never",
      "package-mac": "build -m dmg --publish never",
      "package-win": "build -w nsis --publish never"
    },
    "build": {
      "appId": "org.dainst.idai-field-client",
      "directories": {
        "output": "release"
      },
      "mac": {
        "category": "public.app-category.productivity",
        "target": "zip",
        "icon": "img/logo.icns"
      },
      "win": {
        "target": "zip",
        "icon": "img/logo.ico"
      },
      "npmRebuild": false,
      "extraResources": [
        "imagestore"
      ]
    },
