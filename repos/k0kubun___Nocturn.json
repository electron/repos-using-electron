      "release": "npm run build && npm run upload",
      "setup": "gulp bower && gulp compile-fonts",
      "start": "cross-env NODE_ENV=development npm run compile && electron .",
      "upload": "node script/upload.js",
      "zip": "node script/zip.js"
    },
