      "lint-fix": "npm run lint -- --fix",
      "hot-updates-server": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack/webpack.config.renderer.dev.js",
      "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
      "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack/webpack.config.renderer.dev.dll.js --progress --profile --colors",
