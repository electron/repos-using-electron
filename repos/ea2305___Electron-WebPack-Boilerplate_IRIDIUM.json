      "babel-preset-react": "^6.16.0",
      "cross-env": "^3.1.3",
      "electron": "^1.4.1",
      "electron-compilers": "^5.1.1",
      "electron-packager": "^8.0.0",
      "electron-prebuilt-compile": "^1.4.12",
      "npm-run-all": "^3.1.2"
    },
    "dependencies": {
      "electron-compile": "^5.0.0",
      "react": "^15.4.1",
      "react-dom": "^15.4.1"
    },
