    "scripts": {
      "electron": "electron .",
      "test:lint": "tslint --force 'src/**/*.ts'",
      "test:lint:fail": "tslint 'src/**/*.ts'",
      "test": "npm run test:lint:fail && npm run ionic:build",
      "clear:www": "rimraf ./www/",
      "build:osx:dev": "npm run clear:osx && npm run clear:www && npm run ionic:build:electron && npm run rebuild:osx && npm run pack:osx",
      "build:osx:dist": "npm run clear:osx && npm run clear:www && npm run ionic:build:electron && npm run rebuild:osx  && npm run pack:osx:asar",
      "pack:osx:asar": "npm run pack -- --arch=x64 --platform=darwin --asar",
      "pack:osx": "npm run pack -- --arch=x64 --platform=darwin",
      "clear:osx": "rimraf ./platforms/*-darwin-x64 && rimraf ./platforms/*-mas-x64",
      "rebuild:osx": "electron-rebuild -f -w printer --arch=darwin",
      "pack:win:asar": "npm run pack -- --platform=win32 --arch=ia32 --asar",
      "pack:win": "npm run pack -- --platform=win32 --arch=ia32",
      "build:win:dev": "npm run clear:win && npm run clear:www && npm run ionic:build:electron && npm run rebuild:win && npm run pack:win",
      "build:win:dist": "npm run clear:win && npm run clear:www && npm run ionic:build:electron && npm run rebuild:win && npm run pack:win:asar",
      "clear:win": "rimraf ./platforms/*-win32-ia32",
      "rebuild:printer": "npm rebuild --runtime=electron --target=1.4.10 --arch=ia32",
      "rebuild:win": "npm run rebuild:printer",
      "pack": "electron-packager . --out=platforms --prune --overwrite --ignore=\"(src|setup|platforms|plugins|resources|hooks|.tmp|.idea|.editorconfig|.gitignore|.travis.yml|config.xml|.config.js|tsconfig.json|tslint.json|typings.json)\"",
      "ionic:build": "ionic-app-scripts build --prod",
      "ionic:build:electron": "npm run ionic:build && npm run server:build",
      "ionic:serve": "ionic-app-scripts serve",
      "start": "npm run ionic:serve",
      "start:server": "nodemon --ignore \"*.json\" --exec \"ts-node\" -- \"src/server/index.ts\"",
      "server:build": "tsc -p src/server",
      "migrate:latest": "knex migrate:latest --knexfile src/server/knexfile.ts",
      "migrate:rollback": "knex migrate:rollback --knexfile src/server/knexfile.ts",
      "migrate:reset": "npm run migrate:rollback && npm run migrate:latest",
      "seed:run": "knex seed:run --knexfile src/server/knexfile.ts"
    },
